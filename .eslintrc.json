{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "airbnb-typescript"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/no-danger": "off",
      "jsx-a11y/anchor-is-valid": "off"
    }
  }
  